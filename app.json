{
  "expo": {
    "name": "The Box",
    "slug": "celo-box",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "celobox",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.bakoushin.celobox",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.bakoushin.celobox",
      "versionCode": 1
    },
    "description": "Collect money for any goal using Celo network",
    "githubUrl": "https://github.com/bakoushin/celo-box"
  }
}
